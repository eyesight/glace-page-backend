{
  "kind": "collectionType",
  "collectionName": "receipts",
  "info": {
    "singularName": "receipt",
    "pluralName": "receipts",
    "displayName": "Receipt",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "steps": {
      "type": "component",
      "repeatable": true,
      "component": "food.steps",
      "required": true
    },
    "ingredients": {
      "type": "component",
      "repeatable": true,
      "component": "food.ingredients",
      "required": true
    },
    "portions": {
      "type": "integer",
      "default": 4,
      "required": true
    },
    "optional_Ingredients": {
      "type": "component",
      "repeatable": true,
      "component": "food.ingredients"
    },
    "image": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "categories": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::category.category",
      "mappedBy": "receipts"
    },
    "collections": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::collection.collection",
      "mappedBy": "receipts"
    },
    "likes": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::like.like",
      "mappedBy": "receipts"
    }
  }
}
